extract $
extract obj_props

let hitbox_obj = {
    "block":467,
    "straight":471,
    "corner":469,
    "flat":468,
    "slope":665,
    "flat-slope":666,
}

let graphics = {
    hitbox : (Type="block",pos_x=0,pos_y=0,size=1,rot=0) {
        return $.add(obj {OBJ_ID: hitbox_obj[Type],X: pos_x,Y: pos_y,SCALING: size})
    }
}

return graphics
